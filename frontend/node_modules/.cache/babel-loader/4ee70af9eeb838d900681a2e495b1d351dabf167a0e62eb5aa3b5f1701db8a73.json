{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { computed, reactive } from \"vue\";\nimport axios from \"axios\";\nimport lib from \"@/scripts/lib\";\nimport router from \"@/scripts/router\";\nexport default {\n  setup() {\n    const state = reactive({\n      items: [],\n      form: {\n        name: \"\",\n        address: \"\",\n        payment: \"\",\n        cardNumber: \"\",\n        items: \"\"\n      }\n    });\n    const load = () => {\n      axios.get(\"/api/cart/items\").then(({\n        data\n      }) => {\n        console.log(data);\n        state.items = data;\n      });\n    };\n    const submit = () => {\n      const args = JSON.parse(JSON.stringify(state.form));\n      args.items = JSON.stringify(state.items);\n      axios.post(\"/api/orders\", args).then(() => {\n        alert('주문 완료하였습니다.');\n        router.push({\n          path: \"/orders\"\n        });\n      });\n    };\n    const computedPrice = computed(() => {\n      let result = 0;\n      for (let i of state.items) {\n        result += i.price - i.price * i.discountPer / 100;\n      }\n      return result;\n    });\n    load();\n    return {\n      state,\n      lib,\n      computedPrice,\n      submit\n    };\n  }\n};","map":{"version":3,"names":["computed","reactive","axios","lib","router","setup","state","items","form","name","address","payment","cardNumber","load","get","then","data","console","log","submit","args","JSON","parse","stringify","post","alert","push","path","computedPrice","result","i","price","discountPer"],"sources":["C:\\Users\\My_PC\\Desktop\\SKU\\2023\\gallery\\frontend\\src\\pages\\Order.vue"],"sourcesContent":["<template>\r\n  <div class=\"order\">\r\n    <div class=\"container\">\r\n      <main>\r\n        <div class=\"py-5 text-center\"><h2>주문하기</h2>\r\n          <p class=\"lead\">Below is an example form built entirely with Bootstrap’s form controls. Each required form\r\n            group has a validation state that can be triggered by attempting to submit the form without completing\r\n            it.</p></div>\r\n        <div class=\"row g-5\">\r\n          <div class=\"col-md-5 col-lg-4 order-md-last\"><h4\r\n              class=\"d-flex justify-content-between align-items-center mb-3\">\r\n            <span class=\"text-primary\">구입 목록</span>\r\n            <span\r\n                class=\"badge bg-primary rounded-pill\">\r\n              {{ state.items.length }}\r\n            </span></h4>\r\n            <ul class=\"list-group mb-3\">\r\n              <li class=\"list-group-item d-flex justify-content-between lh-sm\" v-for=\"(i, idx) in state.items\"\r\n                  :key=\"idx\">\r\n                <div>\r\n                  <h6 class=\"my-0\">{{ i.name }}</h6>\r\n                </div>\r\n                <span class=\"text-muted\">\r\n                  {{ lib.getNumberFormatted(i.price - i.price * i.discountPer / 100) }}원\r\n                </span>\r\n              </li>\r\n            </ul>\r\n            <h3 class=\"text-center total-price\">\r\n              {{ lib.getNumberFormatted(computedPrice) }}원\r\n            </h3>\r\n          </div>\r\n          <div class=\"col-md-7 col-lg-8\"><h4 class=\"mb-3\">주문자 정보</h4>\r\n            <div class=\"needs-validation\" novalidate=\"\">\r\n              <div class=\"row g-3\">\r\n                <div class=\"col-12\"><label for=\"username\" class=\"form-label\">이름</label>\r\n                  <input type=\"text\"\r\n                         class=\"form-control\"\r\n                         id=\"username\"\r\n                         v-model=\"state.form.name\"\r\n                  >\r\n                </div>\r\n                <div class=\"col-12\"><label for=\"address\" class=\"form-label\">주소</label>\r\n                  <input type=\"text\"\r\n                         class=\"form-control\"\r\n                         id=\"address\"\r\n                         v-model=\"state.form.address\">\r\n                </div>\r\n              </div>\r\n              <hr class=\"my-4\">\r\n              <h4 class=\"mb-3\">결제 수단</h4>\r\n              <div class=\"my-3\">\r\n                <div class=\"form-check\">\r\n                  <input id=\"card\" name=\"paymentMethod\" type=\"radio\" class=\"form-check-input\"\r\n                         value=\"card\" v-model=\"state.form.payment\">\r\n                  <label class=\"form-check-label\" for=\"card\">신용카드\r\n                  </label></div>\r\n                <div class=\"form-check\">\r\n                  <input id=\"bank\" name=\"paymentMethod\" type=\"radio\" class=\"form-check-input\"\r\n                         value=\"bank\" v-model=\"state.form.payment\">\r\n                  <label class=\"form-check-label\" for=\"bank\">무통장입금</label>\r\n                </div>\r\n              </div>\r\n              <label for=\"cc-name\" class=\"form-label\">카드 번호</label>\r\n              <input type=\"text\"\r\n                     class=\"form-control\"\r\n                     id=\"cc-name\"\r\n                     v-model=\"state.form.cardNumber\">\r\n              <hr class=\"my-4\">\r\n              <button class=\"w-100 btn btn-primary btn-lg\" @click=\"submit()\">결제하기</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </main>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {computed, reactive} from \"vue\";\r\nimport axios from \"axios\";\r\nimport lib from \"@/scripts/lib\";\r\nimport router from \"@/scripts/router\";\r\n\r\nexport default {\r\n  setup() {\r\n    const state = reactive({\r\n      items: [],\r\n      form: {\r\n        name: \"\",\r\n        address: \"\",\r\n        payment: \"\",\r\n        cardNumber: \"\",\r\n        items: \"\",\r\n      }\r\n    })\r\n\r\n    const load = () => {\r\n      axios.get(\"/api/cart/items\").then(({data}) => {\r\n        console.log(data);\r\n        state.items = data;\r\n      })\r\n    };\r\n\r\n    const submit = () => {\r\n      const args = JSON.parse(JSON.stringify(state.form));\r\n      args.items = JSON.stringify(state.items);\r\n\r\n      axios.post(\"/api/orders\", args).then(() => {\r\n        alert('주문 완료하였습니다.');\r\n        router.push({path: \"/orders\"})\r\n      })\r\n    }\r\n\r\n    const computedPrice = computed(() => {\r\n      let result = 0;\r\n\r\n      for (let i of state.items) {\r\n        result += i.price - i.price * i.discountPer / 100;\r\n      }\r\n\r\n      return result;\r\n    })\r\n\r\n    load();\r\n\r\n    return {state, lib, computedPrice, submit}\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n</style>"],"mappings":";AA8EA,SAAQA,QAAQ,EAAEC,QAAQ,QAAO,KAAK;AACtC,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,GAAE,MAAO,eAAe;AAC/B,OAAOC,MAAK,MAAO,kBAAkB;AAErC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,CAAC;MACrBM,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE,EAAE;QACXC,UAAU,EAAE,EAAE;QACdL,KAAK,EAAE;MACT;IACF,CAAC;IAED,MAAMM,IAAG,GAAIA,CAAA,KAAM;MACjBX,KAAK,CAACY,GAAG,CAAC,iBAAiB,CAAC,CAACC,IAAI,CAAC,CAAC;QAACC;MAAI,CAAC,KAAK;QAC5CC,OAAO,CAACC,GAAG,CAACF,IAAI,CAAC;QACjBV,KAAK,CAACC,KAAI,GAAIS,IAAI;MACpB,CAAC;IACH,CAAC;IAED,MAAMG,MAAK,GAAIA,CAAA,KAAM;MACnB,MAAMC,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,SAAS,CAACjB,KAAK,CAACE,IAAI,CAAC,CAAC;MACnDY,IAAI,CAACb,KAAI,GAAIc,IAAI,CAACE,SAAS,CAACjB,KAAK,CAACC,KAAK,CAAC;MAExCL,KAAK,CAACsB,IAAI,CAAC,aAAa,EAAEJ,IAAI,CAAC,CAACL,IAAI,CAAC,MAAM;QACzCU,KAAK,CAAC,aAAa,CAAC;QACpBrB,MAAM,CAACsB,IAAI,CAAC;UAACC,IAAI,EAAE;QAAS,CAAC;MAC/B,CAAC;IACH;IAEA,MAAMC,aAAY,GAAI5B,QAAQ,CAAC,MAAM;MACnC,IAAI6B,MAAK,GAAI,CAAC;MAEd,KAAK,IAAIC,CAAA,IAAKxB,KAAK,CAACC,KAAK,EAAE;QACzBsB,MAAK,IAAKC,CAAC,CAACC,KAAI,GAAID,CAAC,CAACC,KAAI,GAAID,CAAC,CAACE,WAAU,GAAI,GAAG;MACnD;MAEA,OAAOH,MAAM;IACf,CAAC;IAEDhB,IAAI,CAAC,CAAC;IAEN,OAAO;MAACP,KAAK;MAAEH,GAAG;MAAEyB,aAAa;MAAET;IAAM;EAC3C;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}